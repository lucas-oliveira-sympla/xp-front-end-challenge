{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "rules": {
    // A jsx extension is not required for files containing jsx
    "react/jsx-filename-extension": 0,
    "import/no-extraneous-dependencies": 0,
    "react/jsx-props-no-spreading": 0
  }
}
